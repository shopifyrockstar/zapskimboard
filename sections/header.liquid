                },
                {
                    "value": "sticky",
                    "label": "Sticky"
                },
                {
                    "value": "scroll",
                    "label": "Scroll up"
                }
            ],
            "default": "static"
        },
        {
            "type": "checkbox",
            "id": "parent_links",
